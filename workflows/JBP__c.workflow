<?xml version="1.0" encoding="UTF-8"?>
<Workflow xmlns="http://soap.sforce.com/2006/04/metadata">
    <alerts>
        <fullName>JBPApprovalNotification</fullName>
        <description>JBPApprovalNotification</description>
        <protected>false</protected>
        <recipients>
            <type>owner</type>
        </recipients>
        <senderAddress>sfdc.support@hp.com</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
        <template>unfiled$public/JBPApprovalAcceptance</template>
    </alerts>
    <alerts>
        <fullName>JBPApprove_RejectionMail</fullName>
        <description>JBPApprove/RejectionMail</description>
        <protected>false</protected>
        <recipients>
            <type>owner</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>unfiled$public/JBP_PBMNotificationTemplate</template>
    </alerts>
    <alerts>
        <fullName>JBPRejectionMail</fullName>
        <description>JBPRejectionMail</description>
        <protected>false</protected>
        <recipients>
            <type>owner</type>
        </recipients>
        <senderAddress>sfdc.support@hp.com</senderAddress>
        <senderType>OrgWideEmailAddress</senderType>
