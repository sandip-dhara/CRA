        <protected>false</protected>
        <recipients>
            <type>owner</type>
        </recipients>
        <senderType>CurrentUser</senderType>
        <template>unfiled$public/JBP_PBMPartnerApprv_RejctNotification</template>
    </alerts>
    <fieldUpdates>
        <fullName>HP_Approval_Status</fullName>
        <field>HP_Approval_Status__c</field>
        <literalValue>Not Submitted</literalValue>
        <name>HP Approval Status</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>JBPApproveFieldUpdate</fullName>
        <field>HP_Approval_Status__c</field>
        <literalValue>Approved</literalValue>
        <name>JBPApproveFieldUpdate</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
        <reevaluateOnChange>true</reevaluateOnChange>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>JBPInitialFieldUpdate</fullName>
        <field>HP_Approval_Status__c</field>
        <literalValue>Submitted</literalValue>
        <name>JBPInitialFieldUpdate</name>
        <notifyAssignee>false</notifyAssignee>
        <operation>Literal</operation>
        <protected>false</protected>
    </fieldUpdates>
    <fieldUpdates>
        <fullName>JBPRejectionFieldUpdate</fullName>
        <field>HP_Approval_Status__c</field>
        <literalValue>Rejected</literalValue>
        <name>JBPRejectionFieldUpdate</name>
        <notifyAssignee>false</notifyAssignee>
