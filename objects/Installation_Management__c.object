        <url>{!REQUIRESCRIPT (&quot;/soap/ajax/25.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/23.0/apex.js&quot;)} 
var oppId = &quot;{!Opportunity.Id}&quot;; 



var result = sforce.connection.query(&quot;SELECT name from Installation_Management__c WHERE Opportunity__c = &apos;&quot; + oppId + &quot;&apos; limit 1&quot;); 
records = result.getArray(&quot;records&quot;); 
if (records.length &gt;0) 
{ 
alert(&apos;Only one Installation Management can be created per Opportunity&apos;); 
} 
else{ 
parent.window.location.replace(&apos;/{!$ObjectType.Installation_Management__c}/e?CF00NG0000009HGvh={!URLENCODE(Opportunity.Name)}&amp;&amp;CF00NG0000009HGvh_lkid={!Opportunity.Id}&amp;retURL={!Opportunity.Id}&apos;); 
}</url>
