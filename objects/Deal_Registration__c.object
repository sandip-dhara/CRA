    <validationRules>
        <fullName>Deal_Reg_Auto_Approve_DeadLine_check</fullName>
        <active>true</active>
        <description>This validation fires when a L1 approver checks Final Approval flag and enters values in L1 Approval Dead line date field</description>
        <errorConditionFormula>IF(($Setup.Global_Config__c.Mute_Validation_Rule__c==true) , False,
        IF(AND (Final_Approver__c  =  true,   NOT(ISNULL(Level_2_Approval_Deadline__c) ))  ,                        true, false)
)</errorConditionFormula>
        <errorMessage>Both Final Approver and Level 2 Approval Deadline cannot be entered. Choose only one.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Required_For_Product_Registration</fullName>
        <active>false</active>
        <errorConditionFormula>AND(
(Product_Registrations_Count__c  &lt;&gt; 0),
( ISPICKVAL(Status__c,&quot;Submitted&quot;))
)</errorConditionFormula>
        <errorMessage>Please Approve/Reject Product Registrations To Continue</errorMessage>
    </validationRules>
