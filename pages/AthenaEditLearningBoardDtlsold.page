                $('[id$=divVideoImg]').hide();
                
                $('[id$=divVideoPreview]').show();
                
                var srcurl1 = document.getElementById("{!$Component.CreateLearning.txtVideoURL}").value;
                srcurl1 = srcurl1.replace("watch?v=", "embed/");
                srcurl1 = srcurl1.replace("http:", "https:");
                $('#testvideo').attr('src', srcurl1);
                $('#testvideo').reload(true);
                
               
            }
            else if (LType=="Link or Document") {
                debugger;
                var href = $('[id$=LearningLink]').val();
                var link = $('<a href="' + href + '" />');
