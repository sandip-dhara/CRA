            var sampleTags = [{!AvailTags}];
 
 
 
            //-------------------------------
            // Single TAG field
            //-------------------------------
            $('#learningTagsTop').tagit({
                availableTags: sampleTags,
                // This will make Tag-it submit a single form value, as a comma-delimited field.
                singleField: true,
                singleFieldNode: $('#learningTags')
            });
 
            $('#learningTagsTextTop').tagit({
                availableTags: sampleTags,
                // This will make Tag-it submit a single form value, as a comma-delimited field.
                singleField: true,
                singleFieldNode: $('#learningTagsText')
            });
 
            $('#learningTagsLinkTop').tagit({
                availableTags: sampleTags,
                // This will make Tag-it submit a single form value, as a comma-delimited field.
                singleField: true,
                singleFieldNode: $('#learningTagsLink')
            });
 
            $('#learningTagsVideoTop').tagit({
                availableTags: sampleTags,
                // This will make Tag-it submit a single form value, as a comma-delimited field.
                singleField: true,
                singleFieldNode: $('#learningVideoLink')
            });
 
            $(" input[type=radio], input[type=checkbox]").uniform();
 
            //show hide imageUpload in popup
            $("input[type=radio]").change(function () {
                var divId = $(this).attr("id");
                $("div.check").hide();
                $("." + divId).show();
                $("input[type=checkbox]").change();
            });
 
            //toggle the componenet with class menu_body
            $(".menu_body").show();
            $(".menu_head").click(function () {
                $(this).next(".menu_body").slideToggle(600);
                var plusmin;
                plusmin = $(this).children(".plusminus").text();
 
                if (plusmin == '+')
                    $(this).children(".plusminus").text('-');
                else
                    $(this).children(".plusminus").text('+');
            });

            //reade more tags
            $('.show-more').click(function () {
                var tagDisplay = $(this).prev('.tagDisplay');
                if ($(tagDisplay).css('height') != '30px') {
                    $(tagDisplay).stop().animate({ height: '30px' }, 200);
                    $(this).text('More...');
                } else {
                    $(tagDisplay).css({ height: '100%' });
                    var xx = $(tagDisplay).height();
                    $(tagDisplay).css({ height: '30px' });
                    $(tagDisplay).stop().animate({ height: xx }, 400);
                    $(this).text('Less...');
                }
            });
 
 
        });
