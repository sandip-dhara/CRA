        <script type="text/javascript">

            function SubmitSearchForm(event) {
                if (event.keyCode == 13) {
                    var txtSearch = document.getElementById('{!$Component.UPForm.searchText}');
                    if (txtSearch.value.trim().length == 0) {
                        document.getElementById('err_msg').innerHTML = "Enter Value For Search";
                        document.getElementById('err_msg').style.display = 'block';
                        event.preventDefault();
                        event.stopPropagation();
                        return false;
                    }
                    else {
                        document.getElementById('err_msg').style.display = 'none';
                        var button = document.getElementById("{!$Component.UPForm.searchButton}");
                        button.click();
                    }
                    return true;
                }
            }

            //Array for Borad Ids
            var listBoardIds = new Array();
            var listBoardContributeIds = new Array();
            var listBoardFollowIds = new Array();
            var listAllCommunity = new Array();
            function ShowBoardRating(data)
            {
                var BoardID = $(data).attr('data-boardLBid');

                //$('#' + data.id).css("cursor", "default");

                $('#' + BoardID).show();
            }

            function HideBoardRating(data) {
                var BoardID = $(data).attr('data-boardLBid');

                $('#' + BoardID).hide();
            }

            function deleteSpecialChar(ExpertiseTags) {
                console.log('inside delete');
                if (ExpertiseTags.value != '' && ExpertiseTags.value.match(/^[\w ]+$/) == null){
                    ExpertiseTags.value = ExpertiseTags.value.replace(/[\W]/g, '');
                }
            }
            
            var chnageImg = true;
            function loadingIndi(){
                debugger;
                var theImage = new Image();
                if(chnageImg == true){
                    document.getElementById('erroeMsg1').style.display = 'block';
                    return false;
                }
                if(document.querySelector('[id="previewId"] img') == null){
                    document.getElementById('erroeMsg').style.display = 'block';
                    return false;
                }
                theImage.src = document.querySelector('[id="previewId"] img').src;
                var existImage = document.getElementById('{!$Component.formId.demoImage}').src;
                if(existImage == theImage.src){
                    document.getElementById('erroeMsg1').style.display = 'block';
                    return false;
                }
                theImage.onload = function() {
                    console.log('---------'+theImage.height);
                    console.log('---------'+theImage.width);
                };
                var imgf = theImage.type;
                if(theImage.height > 1300 && theImage.width > 2000){
                    document.getElementById('erroeMsg1').style.display = 'none';
                    document.getElementById('erroeMsg').style.display = 'none';
                    document.getElementById('showText').style.display = 'block';
                }
                else{
                     document.getElementById('erroeMsg1').style.display = 'none';
                     document.getElementById('erroeMsg').style.display = 'none';
                     document.getElementById('showText').style.display = 'none';
                    var button = document.getElementById('{!$Component.formId.theButton}');
                    button.click();
                    document.getElementById('showLoadig').style.display = 'block';
                }

            }
            function cancleFun(){
                debugger;
                $('.showHideDiv').hide();
                document.getElementById('hideDiv1').style.display = 'block';
                document.getElementById('LdapRadio').checked = false;
                document.getElementById('BrowseRadio').checked = false;           
                var emptyImg = document.getElementById('{!$Component.formId.demoImage}').src;
                document.getElementById("previewId").innerHTML="<img class='pofilimgS' src='"+emptyImg+"'/>";
                document.getElementById('{!$Component.formId.demoImage1}').src=emptyImg;
                document.getElementById('{!$Component.formId.fileId}').title='No file chosen';
                document.getElementById('nolapImage').style.display = 'none';
                document.getElementById('ldapSavaBtn').disabled = true;  
                document.getElementById('button2').style.display = 'none';
                document.getElementById('button1').style.display = 'block';
            }
            function changeImage(){
                chnageImg = false;
                document.getElementById('erroeMsg1').style.display = 'none';
                document.getElementById('erroeMsg').style.display = 'none';
                document.getElementById('showText').style.display = 'none';
            }
            function LdapImageChg(){
                debugger;
                var Email = document.getElementById('{!$Component.actionId.hiddenEmailId}').value;
                document.getElementById('LdaperroeMsg').style.display = 'none';
                document.getElementById('showLoadig1').style.display = 'block';
                Visualforce.remoting.Manager.invokeAction('{!$RemoteAction.SLP_EditUserProfileController.ldapImageChange}',Email,function(result,event){                              
                    if(result == null){
                        document.getElementById('showLoadig1').style.display = 'none';
                        document.getElementById('nolapImage').style.display = 'block';                      
                        document.getElementById('{!$Component.formId.demoImage1}').src=document.getElementById('{!$Component.actionId.hideImage}').src;
                    }
                    else{
                        document.getElementById('ldapSavaBtn').disabled = false;
                        document.getElementById('{!$Component.formId.demoImage1}').src="data:image/png;base64,"+result; 
                        document.getElementById('showLoadig1').style.display = 'none';
                    }
                },{escape:true});
                return false;
            }
            function Ldapremote(){
                debugger;               
                var srcData = document.getElementById('{!$Component.formId.demoImage1}').src;
                var dupliacteData = document.getElementById('{!$Component.formId.demoImage}').src;
                if(dupliacteData != srcData){ 
                document.getElementById('showLoadig1').style.display = 'block';
                    paraFunction(srcData);
                }
                else{
                    document.getElementById('LdaperroeMsg').style.display = 'block';
                    return false;
                }  
            }
            function DisplayBrowse(){
                document.getElementById('LdaperroeMsg').style.display = 'none';
                document.getElementById('hideDiv2').style.display = 'none';
                document.getElementById('hideDiv1').style.display = 'block';
                document.getElementById('LdapRadio').checked = false;
                document.getElementById('showBrButton').style.display = 'block';
                document.getElementById('button2').style.display = 'none';
                document.getElementById('button1').style.display = 'block';
                document.getElementById('nolapImage').style.display = 'none';
            }
            function DisplayLdap(){
                document.getElementById('hideDiv2').style.display = 'block';
                document.getElementById('hideDiv1').style.display = 'none';
                document.getElementById('BrowseRadio').checked = false;
                document.getElementById('button1').style.display = 'none';
                document.getElementById('button2').style.display = 'block';
                document.getElementById('erroeMsg1').style.display = 'none';
            }
            function reloadPage(){
                debugger;
                window.setTimeout('location.reload()', 20000);
                return true;
            }
