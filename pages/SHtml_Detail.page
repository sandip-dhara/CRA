        $('#hpit_topSearchGo').click(function () {
            var query = $('#hpit_topSearchBox').val();
            var engine = $('#hpit_topSearchScope').val();
            if (engine == "google")
                window.location.href = "http://www.google.com/search?q=" + query;
            else
                window.location.href = "https://search.portal.hp.com/search/simple.htm?query=" + query + "&sid=&x=12&y=15";
        });

